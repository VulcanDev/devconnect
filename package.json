{
  "name": "udemy-project",
  "version": "1.0.0",
  "description": "Social network for devs",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server-dev": "nodemon server.js",
    "client-dev": "npm start --prefix client",
    "dev": "concurrently -c green.bold,cyan.bold npm:server-dev npm:client-dev",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && REACT_ENV_PROD=true npm run build --prefix client"
  },
  "author": "Theo Joyce",
  "license": "MIT",
  "dependencies": {
    "@types/warning": "^3.0.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.14",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  }
}
